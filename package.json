{
  "name": "mui-validate",
  "version": "0.1.0-beta.3",
  "description": "Validation tools for Material UI (core) components and component groups",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "scripts": {
    "build": "rollup -c && rm -r dist/dts",
    "build-watch": "rollup -c -w",
    "test:watch": "jest --watchAll",
    "test": "jest",
    "lint": "eslint . --cache --ext .ts",
    "start-playground": "cd playground && npm run start",
    "i-all": "npm i && cd playground && npm i",
    "dev": "npm-run-all --parallel build-watch start-playground",
    "release": "npm run lint && npm run test && npm run build && node scripts/release.js"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.3",
    "@types/jest": "^26.0.24",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "jest": "26.6.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.58.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.12.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9"
  },
  "keywords": [
    "validation",
    "mui",
    "material-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbrueckner/mui-validate"
  },
  "author": "Kenji Br√ºckner <kenji.brueckner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kbrueckner/mui-validate/issues"
  },
  "homepage": "https://github.com/kbrueckner/mui-validate"
}